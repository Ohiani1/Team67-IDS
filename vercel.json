{
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python",
        "config": {
          "buildCommand": "pip install -r requirements.txt",
          "runtime": "python-3.9",
          "converter": {
            "runtime": "python-3.10.7"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/main.py"
      }
    ]
  }